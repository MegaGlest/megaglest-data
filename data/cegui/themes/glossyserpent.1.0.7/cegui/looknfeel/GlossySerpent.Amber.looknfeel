                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="0.5" type="Height"/>
                            <OperatorDim op="Divide">
                                <ImageDim name="GlossySerpent/EmptyGlyph24" dimension="Height" />
                                <AbsoluteDim value="2" />
                            </OperatorDim>
                        </OperatorDim>
                    </Dim>
                    <Dim type="Width">
                        <ImageDim name="GlossySerpent/EmptyGlyph24" dimension="Width" />
                    </Dim>
                    <Dim type="Height">
                        <ImageDim name="GlossySerpent/EmptyGlyph24" dimension="Height" />
                    </Dim>
                </Area>
