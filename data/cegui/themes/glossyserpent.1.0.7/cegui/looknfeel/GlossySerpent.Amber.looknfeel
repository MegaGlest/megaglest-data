                        <UnifiedDim scale="1" type="RightEdge" />
                        <ImageDim name="GlossySerpent/MultiListRight" dimension="Width" />
                    </OperatorDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="BottomEdge" />
                        <ImageDim name="GlossySerpent/MultiListBottom" dimension="Height" />
                    </OperatorDim>
                </Dim>
            </Area>
        </NamedArea>
        
        <NamedArea name="ItemRenderingAreaHScroll">
            <Area>
                <Dim type="LeftEdge" ><ImageDim name="GlossySerpent/MultiListLeft" dimension="Width" /></Dim>
                <Dim type="TopEdge" ><WidgetDim widget="__auto_listheader__" dimension="BottomEdge" /></Dim>
                <Dim type="RightEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="RightEdge" />
                        <ImageDim name="GlossySerpent/MultiListRight" dimension="Width" />
                    </OperatorDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="BottomEdge" />
                        <WidgetDim widget="__auto_hscrollbar__" dimension="Height" />
                    </OperatorDim>
                </Dim>
            </Area>
        </NamedArea>
        
        <NamedArea name="ItemRenderingAreaVScroll">
            <Area>
                <Dim type="LeftEdge" ><ImageDim name="GlossySerpent/MultiListLeft" dimension="Width" /></Dim>
                <Dim type="TopEdge" ><WidgetDim widget="__auto_listheader__" dimension="BottomEdge" /></Dim>
                <Dim type="RightEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="RightEdge" />
                        <WidgetDim widget="__auto_vscrollbar__" dimension="Width" />
                    </OperatorDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="BottomEdge" />
                        <ImageDim name="GlossySerpent/MultiListBottom" dimension="Height" />
                    </OperatorDim>
                </Dim>
            </Area>
        </NamedArea>
        
        <NamedArea name="ItemRenderingAreaHVScroll">
            <Area>
                <Dim type="LeftEdge" ><ImageDim name="GlossySerpent/MultiListLeft" dimension="Width" /></Dim>
                <Dim type="TopEdge" ><WidgetDim widget="__auto_listheader__" dimension="BottomEdge" /></Dim>
                <Dim type="RightEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="RightEdge" />
                        <WidgetDim widget="__auto_vscrollbar__" dimension="Width" />
                    </OperatorDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="BottomEdge" />
                        <WidgetDim widget="__auto_hscrollbar__" dimension="Height" />
                    </OperatorDim>
                </Dim>
            </Area>
        </NamedArea>
        
        <Child  type="GlossySerpent/ListHeader" nameSuffix="__auto_listheader__">
            <Area>
                <Dim type="LeftEdge" ><AbsoluteDim value="4" /></Dim>
                <Dim type="TopEdge" ><AbsoluteDim value="3" /></Dim>
                <Dim type="RightEdge" >
                    <UnifiedDim scale="1" offset="-4" type="RightEdge" />
                </Dim>
                <Dim type="Height" >
                    <OperatorDim op="Multiply">
                        <FontDim type="LineSpacing" />
                        <AbsoluteDim value="1.5" />
                    </OperatorDim>
                </Dim>
            </Area>
        </Child>
        
        <Child  type="GlossySerpent/HorizontalScrollbar" nameSuffix="__auto_hscrollbar__">
            <Area>
                <Dim type="LeftEdge" ><AbsoluteDim value="6" /></Dim> <!-- border thickness: 6-->
                <Dim type="TopEdge" ><AbsoluteDim value="-6" /></Dim>
                <Dim type="Width" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="Width" />
                        <OperatorDim op="Add">
                            <PropertyDim name="ScrollBarPreferredSize" />
                            <AbsoluteDim value="6" />
                        </OperatorDim>
                    </OperatorDim>
                </Dim>
                <Dim type="Height" >
                    <PropertyDim name="ScrollBarPreferredSize" />
                </Dim>
            </Area>
            <VertAlignment type="BottomAligned" />
        </Child>
        
        <Child  type="GlossySerpent/VerticalScrollbar" nameSuffix="__auto_vscrollbar__">
            <Area>
                <Dim type="LeftEdge" ><AbsoluteDim value="-6"  /></Dim>
                <Dim type="TopEdge" >
                    <OperatorDim op="Add">
                        <AbsoluteDim value="6" />
                        <PropertyDim name="ScrollBarPreferredSize" />
                    </OperatorDim>
                </Dim>
                <Dim type="Width" >
                    <PropertyDim name="ScrollBarPreferredSize" />
                </Dim>
                <Dim type="Height" >
                    <OperatorDim op="Add">
                        <UnifiedDim scale="1" offset="-6" type="Height" />
                        <OperatorDim op="Subtract">
                            <AbsoluteDim value="0" />
                            <OperatorDim op="Multiply" >
                                <PropertyDim name="ScrollBarPreferredSize" />
                                <AbsoluteDim value="2" />
                            </OperatorDim>
                        </OperatorDim>
                    </OperatorDim>
                </Dim>
            </Area>
            <HorzAlignment type="RightAligned" />
        </Child>
        <ImagerySection name="main">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <Image component="TopLeftCorner" name="GlossySerpent/MultiListTopLeft" />
                <Image component="TopRightCorner" name="GlossySerpent/MultiListTopRight" />
                <Image component="BottomLeftCorner" name="GlossySerpent/MultiListBottomLeft" />
                <Image component="BottomRightCorner" name="GlossySerpent/MultiListBottomRight" />
                <Image component="LeftEdge" name="GlossySerpent/MultiListLeft" />
                <Image component="RightEdge" name="GlossySerpent/MultiListRight" />
                <Image component="TopEdge" name="GlossySerpent/MultiListTop" />
                <Image component="BottomEdge" name="GlossySerpent/MultiListBottom" />
                <Image component="Background" name="GlossySerpent/MultiListBackdrop" />
            </FrameComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="main" />
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="main">
                    <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
                </Section>
            </Layer>
        </StateImagery>
    </WidgetLook>



