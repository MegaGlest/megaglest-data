        <ImagerySection name="caption">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge" >
                        <OperatorDim op="Add">
                            <ImageDim name="GlossySerpent/TitlebarBorder_L" dimension="Width" />
                            <ImageDim name="GlossySerpent/EmptyGlyph24" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" offset="-10" type="Width" />
                            <UnifiedDim scale="1" type="Height" />
                        </OperatorDim>
                    </Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <ColourProperty name="CaptionColour" />
                <VertFormat type="CentreAligned" />
            </TextComponent>
        </ImagerySection>

        <ImagerySection name="glyph">
